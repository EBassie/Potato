          <loc key="Team"   value="Team" />
          <loc key="Squad"  value="Squad" />
          <loc key="Player" value="Player" />
        </player>
        <time>
          <loc key="Permanent" value="Permanent" />
          <loc key="Round"     value="Round" />
          <loc key="Time"      value="Time" />
        </time>
      </subset>

      <filter>
        <type>
          <loc key="Contains" value="Contains" />
          <loc key="Excludes" value="Excludes" />
          <loc key="Matches"  value="Matches" />
        </type>
        <chat>
          <loc key="Time"      value="Time" />
          <loc key="Type"      value="Type" />
          <loc key="Sender"    value="Sender" />
          <loc key="Recipient" value="Recipient" />
          <loc key="Data"      value="Data" />
        </chat>
        <ban>
          <loc key="Id"     value="Id" />
          <loc key="Time"   value="Time" />
          <loc key="Reason" value="Reason" />
        </ban>
      </filter>

      <action>
        <chat>
          <loc key="Say"      value="Say" />
          <loc key="Yell"     value="Say And Yell" />
          <loc key="YellOnly" value="Yell" />
        </chat>
        <player>
          <loc key="Move" value="Move" />
          <loc key="Kick" value="Kick" />
          <loc key="Kill" value="Kill" />
          <loc key="Ban"  value="Ban" />
        </player>
        <map>
          <loc key="Add"    value="Add To List" />
          <loc key="Remove" value="Remove From List" />
          <loc key="Up"     value="Move Up List" />
          <loc key="Down"   value="Move Down List" />
        </map>
        <ban>
          <loc key="Ban"         value="Ban" />
          <loc key="Unban"       value="Un-ban" />
          <loc key="ToPermanent" value="Convert To Permanent" />
          <loc key="ToTemporary" value="Convert To Temporary" />
        </ban>
      </action>
    </api>

    <!--
      Manager UI Localization.
      
      Contains localization for the Manager UI.  This really shouldn't be
      messed with by anyone unless they're tweaking the manager.
    -->
    <manager>
      <loc key="Assemblies"        value="Assembly Information"/>
      <loc key="AssemblyList"      value="Assemblies Found:"/>
      <loc key="AssemblyInfo"      value="Selected Assembly Information:"/>
      <loc key="AssemblyName"      value="File Name:"/>
      <loc key="AssemblyExtCount"  value="Extensions:"/>
      <loc key="AssemblyConflicts" value="Assembly Level Conflicts:"/>

      <loc key="Extensions"           value="Extension Information"/>
      <loc key="ExtensionList"        value="Extensions Found:"/>
      <loc key="ExtensionInfo"        value="Selected Extension Information:"/>
      <loc key="ExtensionAuthor"      value="Author:"/>
      <loc key="ExtensionLink"        value="Link:"/>
      <loc key="ExtensionName"        value="Name:"/>
      <loc key="ExtensionVersion"     value="Version:"/>
      <loc key="ExtensionDescription" value="Description:"/>
      <loc key="ExtensionConflicts"   value="Extension Level Conflicts:"/>

      <loc key="Save"  value="Save"/>
      <loc key="Close" value="Close"/>
    </manager>

