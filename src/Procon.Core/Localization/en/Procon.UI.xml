    <!--
      Universal API localization.
      
      Generally, localization for enums, constants, or other known values
      ahead of time are localized here.  This is to make it easier for
      extensions to access commonly used enums or constants.
    -->
    <api>
      <game>
        <loc key="None"      value="None"/>
        <loc key="BF_BC2"    value="Battlefield: Bad Company 2"/>
        <loc key="BF_3"      value="Battlefield 3"/>
        <loc key="COD_BO"    value="Call of Duty: Black Ops"/>
        <loc key="MOH_2010"  value="Medal of Honor 2010"/>
        <loc key="HOMEFRONT" value="Homefront"/>
      </game>

      <role>
        <loc key="Assault"    value="Assault" />
        <loc key="Demolition" value="Engineer" />
        <loc key="Recon"      value="Recon" />
        <loc key="Support"    value="Medic" />
      </role>

      <event>
        <loc key="Join"  value="Joined" />
        <loc key="Leave" value="Left" />
