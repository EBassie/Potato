    <!--
      Various Server Variable Localization
      
      Server variable localizations are among the most frequently updated
      localizations simply because there are so many.  The keys are defined
      in code rather than through the UI, is what makes server variable
      localizations special.
    -->
    <variables>
      <loc key="MAX_CONSOLE_LINES"  value="Maximum Console Lines" />
      <loc key="SERVER_NAME"        value="Server Name" />
      <loc key="SERVER_DESCRIPTION" value="Server Description" />
      <loc key="BANNER_URL"         value="Banner Url" />
      <loc key="RANKED"             value="Is Ranked" />
      <loc key="ANTI_CHEAT"         value="Is Anti-Cheat On" />
      <loc key="AUTO_BALANCE"       value="Is Auto Balance On" />
      <loc key="FRIENDLY_FIRE"      value="Is Friendly Fire On" />
      <loc key="PASSWORDED"         value="Is Passworded" />
      <loc key="PASSWORD"           value="Server Password" />
        
      <loc key="MAX_CONSOLE_LINES_Description"  value="The maximum amount of characters that can be entered via the Procon chat window.  This value defaults to a safe value for each game type the Procon supports." />
