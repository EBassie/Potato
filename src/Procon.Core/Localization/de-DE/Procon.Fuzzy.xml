              <augustmonthsvariabletemporalprimitivetoken>
                <match text="August"/>
                <match text="Aug"/>
              </augustmonthsvariabletemporalprimitivetoken>
              <septembermonthsvariabletemporalprimitivetoken>
                <match text="September"/>
                <match text="Sept"/>
              </septembermonthsvariabletemporalprimitivetoken>
              <octobermonthsvariabletemporalprimitivetoken>
                <match text="Oktober"/>
                <match text="Okt"/>
              </octobermonthsvariabletemporalprimitivetoken>
              <novembermonthsvariabletemporalprimitivetoken>
                <match text="November"/>
                <match text="Nov"/>
              </novembermonthsvariabletemporalprimitivetoken>
              <decembermonthsvariabletemporalprimitivetoken>
