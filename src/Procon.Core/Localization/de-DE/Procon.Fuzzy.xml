              </sundaydaysvariabletemporalprimitivetoken>
              <mondaydaysvariabletemporalprimitivetoken>
                <match text="Montag"/>
                <match text="Mo"/>
              </mondaydaysvariabletemporalprimitivetoken>
              <tuesdaydaysvariabletemporalprimitivetoken>
                <match text="Dienstag"/>
                <match text="Di"/>
              </tuesdaydaysvariabletemporalprimitivetoken>
              <wednesdaydaysvariabletemporalprimitivetoken>
                <match text="Donnerstag"/>
