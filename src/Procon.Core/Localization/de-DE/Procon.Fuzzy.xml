        -->
      </primitive>
      <syntax>
        <articles>
          <indefinitearticlessyntaxtoken>
            <match text="a"/>
            <match text="an"/>
          </indefinitearticlessyntaxtoken>
          <definitearticlessyntaxtoken>
            <match text="the"/>
          </definitearticlessyntaxtoken>
        </articles>
        <adjectives>
          <lastadjectivesyntaxtoken>
            <match text="last"/>
          </lastadjectivesyntaxtoken>
          <nextadjectivesyntaxtoken>
            <match text="next"/>
          </nextadjectivesyntaxtoken>
          <thisadjectivesyntaxtoken>
            <match text="this"/>
          </thisadjectivesyntaxtoken>
        </adjectives>
        <punctuation>
          <parentheses>
            <openparenthesespunctuationsyntaxtoken>
              <match text="("/>
            </openparenthesespunctuationsyntaxtoken>
            <closedparenthesespunctuationsyntaxtoken>
              <match text=")"/>
            </closedparenthesespunctuationsyntaxtoken>
          </parentheses>
          <questionmarkpunctuationsyntaxtoken>
            <match text="?"/>
          </questionmarkpunctuationsyntaxtoken>
          <periodpunctuationsyntaxtoken>
            <match text="."/>
