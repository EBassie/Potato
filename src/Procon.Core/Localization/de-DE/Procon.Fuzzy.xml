        <typography>
          <plustypographysyntaxtoken>
            <match text="+"/>
          </plustypographysyntaxtoken>
          <asterisktypographysyntaxtoken>
            <match text="*"/>
          </asterisktypographysyntaxtoken>
        </typography>
      </syntax>
      <reduction>
        <allinclusivereductiontoken>
          <match text="jeder"/>
        </allinclusivereductiontoken>
      </reduction>
    </tokens>
  </Fuzzy>
</procon>