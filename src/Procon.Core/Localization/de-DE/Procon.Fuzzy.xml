<?xml version="1.0" encoding="utf-8" ?>
<?ietf-language-tag de-DE?>
<!DOCTYPE procon [
  <!ATTLIST match regex CDATA #IMPLIED>
  <!ATTLIST match text CDATA #IMPLIED>
]>
<procon>
  <Fuzzy>
    <tokens>
      <tokenreflection>
        <diacritic key="ü" value="ue"/>
        <diacritic key="ä" value="ae"/>
        <diacritic key="ö" value="oe"/>
        <diacritic key="ß" value="ss"/>
        <diacritic key="Ä" value="Ae"/>
        <diacritic key="Ü" value="Ue"/>
        <diacritic key="Ö" value="Oe"/>
      </tokenreflection>
      <object>
        <properties>
          <property name="Name">
            <match text="name"/>
            <match text="spielername"/>
          </property>
          <property name="Ping">
            <match text="ping"/>
          </property>
          <property name="Score">
            <match text="punkte"/>
          </property>
          <property name="Country">
            <match text="land"/>
          </property>
          <property name="Kills">
            <match text="kills"/>
          </property>
          <property name="Deaths">
            <match text="tode"/>
          </property>
          <property name="Kdr">
            <match text="kdr"/>
            <match text="ratio"/>
          </property>
        </properties>

        <selfreflectionthingobjecttoken>
          <match text="mich"/>
          <match text="mein"/>
          <match text="habe ich"/>
          <match text="werde ich"/>
          <match text="bin ich"/>
          <match text="ich"/>
        </selfreflectionthingobjecttoken>
      </object>

      <operator>
        <logical>
          <equality>
            <equalsequalitylogicaloperatortoken>
              <match text="="/>
              <match text="=="/>
              <match text="gleich"/>
              <match text="ist gleich"/>
              <match text="ist gleich"/>
              <match text="gl"/>
            </equalsequalitylogicaloperatortoken>
            <greaterthanequalitylogicaloperatortoken>
              <match text="größer als"/>
              <match text=">"/>
              <match text="über"/>
              <match text="größer als"/>
              <match text="mehr als"/>
              <match text="über"/>
              <match text="gr"/>
            </greaterthanequalitylogicaloperatortoken>
            <greaterthanequaltoequalitylogicaloperatortoken>
              <match text=">="/>
              <match text="gteq"/>
            </greaterthanequaltoequalitylogicaloperatortoken>
            <lessthanequalitylogicaloperatortoken>
              <match text="weniger als"/>
              <match text="&lt;"/>
              <match text="weniger als"/>
              <match text="kleiner als"/>
              <match text="unter"/>
              <match text="kl"/>
            </lessthanequalitylogicaloperatortoken>
            <lessthanequaltoequalitylogicaloperatortoken>
              <match text="&lt;="/>
              <match text="lteq"/>
            </lessthanequaltoequalitylogicaloperatortoken>
          </equality>
          <andlogicaloperatortoken>
            <match text="und"/>
            <match text="in"/>
          </andlogicaloperatortoken>
          <excludinglogicaloperatortoken>
            <match text="ohne"/>
            <match text="ohne"/>
            <match text="oder"/>
            <match text="oder nicht"/>
            <match text="oder nicht in"/>
            <match text="außer"/>
            <match text="nicht in"/>
            <match text="außerhalb"/>
          </excludinglogicaloperatortoken>
          <notlogicaloperatortoken>
            <match text="nicht"/>
          </notlogicaloperatortoken>
          <orlogicaloperatortoken>
            <match text="oder"/>
          </orlogicaloperatortoken>
        </logical>
        <arithmetic>
          <secondorder>
            <divisionsecondorderarithmetictoken>
              <match text="divideren"/>
              <match text="dividieren durch"/>
              <match text="dividert"/>
              <match text="dividiert durch"/>
            </divisionsecondorderarithmetictoken>
            <multiplicationsecondorderarithmetictoken>
              <match text="mal"/>
              <match text="x"/>
              <match text="multiplizieren"/>
              <match text="multiplizieren mit"/>
              <match text="multipliziert"/>
              <match text="multipliziert mit"/>
            </multiplicationsecondorderarithmetictoken>
          </secondorder>
          <thirdorder>
            <additionthirdorderarithmeticoperatortoken>
              <match text="hinzufügen"/>
              <match text="plus"/>
              <match text="kombinieren"/>
              <match text="summe"/>
            </additionthirdorderarithmeticoperatortoken>
            <subtractionthirdorderarithmeticoperatortoken>
              <match text="nehmen"/>
              <match text="minus"/>
            </subtractionthirdorderarithmeticoperatortoken>
          </thirdorder>
        </arithmetic>
      </operator>
      <primitive>
        <numeric>
          <floatnumericprimitivetoken>
            <match regex="^(?&lt;value&gt;[-+]?[0-9]+[ ]?[\.,]?[ ]?[0-9]*)$"/>
            <match text="Paar" value="2"/>
            <match text="ein Paar" value="2"/>
            <match text="Duzend" value="12"/>
            <match text="ein Duzend" value="12"/>

            <match text="Null" value="0"/>
            <match text="Eins" value="1"/>
            <match text="Zwei" value="2"/>
            <match text="Drei" value="3"/>
            <match text="Vier" value="4"/>
            <match text="Fünf" value="5"/>
            <match text="Sechs" value="6"/>
            <match text="Sieben" value="7"/>
            <match text="Acht" value="8"/>
            <match text="Neun" value="9"/>
            <match text="Elf" value="11"/>
            <match text="Zwölf" value="12"/>
            <match text="Dreizehn" value="13"/>
            <match text="Vierzehn" value="14"/>
            <match text="Fünfzehn" value="15"/>
            <match text="Sechzehn" value="16"/>
            <match text="Siebzehn" value="17"/>
            <match text="Achtzehn" value="18"/>
            <match text="Neunzehn" value="19"/>
            <match text="Zehn" value="10"/>
            <match text="Zwanzig" value="20"/>
            <match text="Dreißig" value="30"/>
            <match text="Vierzig" value="40"/>
            <match text="Fünfzig" value="50"/>
            <match text="Sechzig" value="60"/>
            <match text="Siebzig" value="70"/>
            <match text="Achtzig" value="80"/>
            <match text="Neunzig" value="90"/>
          </floatnumericprimitivetoken>
          <ordinalnumericprimitivetoken>
            <match regex="^(?&lt;value&gt;[0-9]*)\'?(st|nd|th)$"/>
          </ordinalnumericprimitivetoken>
          <cardinal>
            <multiplicandcardinalnumericprimitivetoken>
              <match text="Hundert" value="100"/>
              <match text="Tausend" value="1000"/>
              <match text="Million" value="1000000"/>
            </multiplicandcardinalnumericprimitivetoken>
          </cardinal>
        </numeric>
        <temporal>
          <units>
            <meridiem>
              <postmeridiemunitstemporalprimitivetoken>
                <match text="p.m"/>
                <match text="pm"/>
              </postmeridiemunitstemporalprimitivetoken>
              <antemeridiemunitstemporalprimitivetoken>
                <match text="a.m"/>
                <match text="am"/>
              </antemeridiemunitstemporalprimitivetoken>
            </meridiem>
            <daysunittemporalprimitivetoken>
              <match text="Tag"/>
              <match text="Tage"/>
            </daysunittemporalprimitivetoken>
            <hoursunittemporalprimitivetoken>
              <match text="Stunde"/>
              <match text="Stunden"/>
              <match text="Std"/>
            </hoursunittemporalprimitivetoken>
            <minutesunittemporalprimitivetoken>
              <match text="Minute"/>
              <match text="Minuten"/>
              <match text="Min"/>
            </minutesunittemporalprimitivetoken>
            <monthsunittemporalprimitivetoken>
              <match text="Monat"/>
              <match text="Monate"/>
            </monthsunittemporalprimitivetoken>
            <secondsunittemporalprimitivetoken>
              <match text="Sekunde"/>
              <match text="Sekunden"/>
              <match text="Sek"/>
