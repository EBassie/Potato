            </days>
            <months>
              <januarymonthsvariabletemporalprimitivetoken>
                <match text="Januar"/>
                <match text="Jan"/>
              </januarymonthsvariabletemporalprimitivetoken>
              <februarymonthsvariabletemporalprimitivetoken>
                <match text="Februar"/>
                <match text="Feb"/>
              </februarymonthsvariabletemporalprimitivetoken>
              <marchmonthsvariabletemporalprimitivetoken>
                <match text="März"/>
                <match text="Mär"/>
              </marchmonthsvariabletemporalprimitivetoken>
              <aprilmonthsvariabletemporalprimitivetoken>
                <match text="April"/>
                <match text="Apr"/>
              </aprilmonthsvariabletemporalprimitivetoken>
              <maymonthsvariabletemporalprimitivetoken>
                <match text="Mai"/>
